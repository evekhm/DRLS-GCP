#!/usr/bin/env bash
BIN="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
PWD=$(pwd)
source "$BIN"/SET

cd "$BIN"/../../../"$APPLICATION" || exit

echo "Building image $IMAGE..."
rm -rf target
rm -rf build

docker build -f Dockerfile.dev -t "$IMAGE" .

echo "Created image $IMAGE..."

cd "$PWD" || exit