#!/usr/bin/env bash
BIN="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
PWD=$(pwd)

source "$BIN"/../../shared/SET

APPLICATION='keycloak'

cd "$BIN"/../.. || exit
VERSION=$(git log -1 --pretty=%h)
IMAGE="gcr.io/$PROJECT_ID/$APPLICATION"
TAG="$IMAGE:$VERSION"
LATEST="$IMAGE:latest"

cd "$PWD" || exit
