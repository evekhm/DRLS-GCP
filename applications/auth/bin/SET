#!/usr/bin/env bash
BIN="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
PWD=$(pwd)

APPLICATION='keycloak'
source "$BIN"/../../../shared/SET

# Using the locally build image since it needs changes depending on Service Host IP
#cd "$BIN"/../.. || exit
#VERSION=$(git log -1 --pretty=%h)

IMAGE_REPO="gcr.io/$PROJECT_ID/$APPLICATION"
VERSION=0.1
LATEST="${IMAGE_REPO}:latest"

IMAGE=${IMAGE:-"${IMAGE_REPO}:${VERSION}"}


#cd "$PWD" || exit
