#!/usr/bin/env bash
BIN="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
PWD=$(pwd)

source "$BIN"/../../shared/SET

APPLICATION='crd'

cd "$BIN"/../../../CRD || exit
VERSION=$(git log -1 --pretty=%h)
IMAGE="$REPO/$APPLICATION"
TAG="$IMAGE:$VERSION"
LATEST="$IMAGE:latest"

BUCKET_NAME=${PROJECT_ID}-${APPLICATION}
BUCKET="gs://$BUCKET_NAME"

cd "$PWD" || exit