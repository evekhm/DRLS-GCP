#!/usr/bin/env bash
BIN="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
source "$BIN"/../../shared/SET

APPLICATION='crd'
VERSION=v5
IMAGE="gcr.io/$PROJECT_ID/$APPLICATION:$VERSION"

##############  Application Specific Parameters  ##############
LOCAL_PATH="$BIN/../../../CRD/CDS-Library.zip" #To upload to GCP for crd service

#  GCP Storage
BUCKET_NAME=${PROJECT_ID}-${APPLICATION}
BUCKET="gs://$BUCKET_NAME"
DB='CDS-Library.zip'