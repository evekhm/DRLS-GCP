#!/usr/bin/env bash
BIN="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
PWD=$(pwd)

source "$BIN"/../../shared/SET

APPLICATION='dtr'

cd "$BIN"/../../../"$APPLICATION" || exit
VERSION=$(git log -1 --pretty=%h)

IMAGE="$REPO/$APPLICATION"
TAG="$IMAGE:$VERSION"
LATEST="$IMAGE:latest"

cd "$PWD" || exit
